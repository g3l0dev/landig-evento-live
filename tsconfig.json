{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@const/*": [
        "src/const/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  }
}